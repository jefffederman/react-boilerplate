{
  "name": "react-boilerplate",
  "version": "0.2.0",
  "description": "Quick packager-agnostic boilerplate for React modules using JSX",
  "main": "lib/index.js",
  "dependencies": {
    "react": "latest",
    "statics-stylesheets": "latest",
    "stylesheets": "latest"
  },
  "devDependencies": {
    "jsxc": "latest"
  },
  "scripts": {
    "start": "jsxc --watch src/ lib/",
    "build": "jsxc src/ lib/"
  },
  "files": ["lib/", "USAGE.txt", "static/"],
  "staticRoot": "./static/",
  "staticPlugin": "statics-stylesheets",
  "author": "Jeff Federman",
  "license": "MIT"
}
